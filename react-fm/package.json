{
  "name": "react-fm",
  "version": "1.0.0",
  "homepage": "https://stag.flickmatch.in/",
  "description": "Frontend for Flickmatch.in",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "https-preview": "serve dist",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "gh-pages:404": "cp ./404.html ./dist",
    "add-domain": "echo play.flickmatch.in > dist/CNAME",
    "predeploy": "npm run build && npm run gh-pages:404 && npm run add-domain",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.6",
    "@mui/lab": "5.0.0-alpha.100",
    "@mui/material": "^5.10.6",
    "@mui/system": "^5.10.6",
    "@mui/utils": "^5.10.6",
    "@react-oauth/google": "^0.12.1",
    "@types/gapi.client": "^1.0.8",
    "axios": "^1.6.5",
    "gapi-client": "^0.0.3",
    "is-mobile": "^3.1.1",
    "lodash": "^4.17.21",
    "notistack": "^2.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-ga": "^3.3.1",
    "react-helmet-async": "^1.3.0",
    "react-hotkeys-hook": "^3.4.7",
    "react-material-ui-carousel": "^3.4.2",
    "react-router-dom": "^6.4.1",
    "react-youtube": "^10.1.0",
    "recoil": "^0.7.5"
  },
  "devDependencies": {
    "@iconify/react": "^4.1.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/lodash": "^4.14.197",
    "@types/node": "^18.7.21",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/youtube-player": "^5.5.9",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitejs/plugin-react": "^2.1.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-autofix": "^1.1.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^5.0.0",
    "https-localhost": "^4.7.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.63.6",
    "typescript": "^4.8.3",
    "vite": "^3.2.8",
    "vite-plugin-pwa": "^0.13.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "npx prettier . --write",
      "npx eslint ."
    ]
  },
  "author": {
    "name": "Rohan Sood",
    "email": "rohans@flickmatch.in",
    "url": "http://flickmatch.in/"
  }
}
